const triggerWord = "cancun";
const alertMessage = "Not a chance...\n\nYou found the secret word. Cancun. Really didn't expect anyone to find this, so congratulations.\n\nHonestly, I didn't realize how spineless Ted Cruz is/was when I first started working on this mod, and I'm saying that as a Republican.\n\nThis so-called 'patriot' abandoned his constituents in Texas and fled to Mexico, then proceeds to criticize others for doing the same thing. He pretty much spends his career bowing down to whatever Trump wants and being an AIPAC suckup.\n\nBut you can't deny that he's fun to make a mod on. Congratulations, you just got an achievement.";
let keyBuffer = "";

function checkTrigger(value) {
    if (value.toLowerCase().includes(triggerWord.toLowerCase())) {
        alert(alertMessage);
        ctsAchievement("Cancun");
        return true;
    }
    return false;
}

document.addEventListener("keydown", function(event) {
    const activeElement = document.activeElement;
    
    if ((activeElement.tagName === "INPUT" || activeElement.tagName === "TEXTAREA") && event.key === "Enter") {
        const typedValue = activeElement.value.trim();
        if (checkTrigger(typedValue)) {
            activeElement.value = "";
        }
    } else if (event.key.length === 1) {
        keyBuffer += event.key;
    } else if (event.key === "Backspace") {
        keyBuffer = keyBuffer.slice(0, -1);
    } else if (event.key === "Enter") {
        if (checkTrigger(keyBuffer)) {}
        keyBuffer = "";
    }
});


// Not gonna lie, I have no idea who made this. Think it was Onkel Danny, not sure. Shoutout to whoever made this.

construct = (a = 1) => {
    
    e.page = e.page + a < e.pages.length ? e.page + a : 0
    let html = e.header;
    html += e.pages[e.page] + "<br>";
    
    if (e.page > 0) {
        html += `<button onclick='endingConstructor(a = -1)'>Back</button>`;
    }
    if (e.page < e.pages.length - 1) {
        html += `<button onclick='endingConstructor(a = 1)'>Next</button>`;
    }
    
    for (i in e.executable) {
        if (e.executable[i][0] == e.page) {
            e.executable[i][1]();
        }
    }
    
if (e.image === true) {
    setTimeout(() => {
        let candImg = $(".person_image")[0];
        if (candImg) {
            candImg.remove();
            $("#final_results_description")[0].style = `
                display: block;
                margin-left: auto;
                margin-right: auto;
                color: white;
            `;
        }
    }, 10);
} else if (e.image) {
    setTimeout(() => {
        let candImg = $(".person_image")[0];
        if (candImg) {
            candImg.src = e.image;
            $("#final_results_description")[0].style = `
                display: block;
                margin-left: auto;
                margin-right: auto;
                color: white; /* Keep all text white */
            `;
        }       
    }, 1);
}

    return html;
}

endingConstructor = (a = 1) => {
    $("#final_results_description")[0].innerHTML = construct(a);
}

e.multiple_endings = true;

e.header = "<h1>Something broke!</h1>";
e.pages = ["<p>This should not be showing. If it did, then my mod broke. Contact @theMcCain on Discord. Congrats if you won, if you lost, better luck next time</p>"];
e.page = 0;

function ctsAchievement(achievement, difficultyChecker = true){
    if ((difficultyChecker && campaignTrail_temp.difficulty_level_multiplier<=1)||!difficultyChecker)
        if(campaignTrail_temp.CTS){
            unlockAchievement(achievement);
        }
}

endingPicker = (out, totv, aa, quickstats) => {
    orderID = [aa[0].candidate, aa[1].candidate, aa[2].candidate, aa[3].candidate];
    
